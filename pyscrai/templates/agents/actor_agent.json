{
    "name": "Actor Agent",
    "description": "A template for a basic actor agent that plays a character in the scenario",
    "engine_type": "actor",
    "personality_config": {
        "role": "character_performer",
        "backstory": "An adaptable actor that can take on different character roles",
        "goals": ["portray characters authentically", "create engaging interactions"],
        "traits": {
            "adaptability": "high",
            "creativity": "high",
            "empathy": "medium"
        },
        "instructions": ["Respond in character", "Maintain consistent personality"],
        "constraints": ["Don't break character"]
    },
    "llm_config": {
        "provider": "openrouter",
        "model_id": "meta-llama/llama-4-maverick:free",
        "temperature": 0.8,
        "top_p": 0.95,
        "max_tokens": 800
    },
    "tools_config": {
        "memory": {
            "name": "memory",
            "enabled": true,
            "config": {
                "max_entries": 30
            }
        },
        "character_profile": {
            "name": "character_profile",
            "enabled": true,
            "config": {
                "traits": ["adaptable", "resourceful"]
            }
        }
    },
    "runtime_overrides": {
        "personality_config": {
            "traits": "merge_allowed",
            "goals": "append_allowed"
        },
        "llm_config": {
            "temperature": "override_allowed"
        }
    },
    "engine_specific_config": {
        "actor": {
            "character_reasoning": true,
            "context_window": 20,
            "emotional_modeling": true,
            "relationship_tracking": true
        }
    }
}
